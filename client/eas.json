{
    "cli": {
        "version": ">= 16.14.1",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "SENTRY_PROJECT": "patientdashboardapp",
                "SENTRY_ORG": "na-cci"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "SENTRY_PROJECT": "patientdashboardapp",
                "SENTRY_ORG": "na-cci"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "SENTRY_PROJECT": "patientdashboardapp",
                "SENTRY_ORG": "na-cci"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
